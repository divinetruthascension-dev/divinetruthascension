    "preview": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "preact": "^10.27.0",
    "preact-render-to-string": "^6.5.13",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "cross-env": "^10.0.0",
    "typescript": "~5.9.2",
    "vite": "^7.0.6"
  }
}
